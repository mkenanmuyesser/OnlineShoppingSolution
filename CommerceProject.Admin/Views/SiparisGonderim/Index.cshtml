@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MainRoot.cshtml";
}

<div class="portlet light bordered blue-hoki">
    <div class="portlet-title">
        <div class="caption">
            <i class="icon-puzzle font-white"></i>
            <span class="caption-subject bold font-white uppercase"> @ViewBag.PageProperties.PageHeader </span>
            <span class="caption-helper">@ViewBag.PageProperties.PageDescription</span>
        </div>
        <div class="btn-group pull-right">
            <button type="button" class="btn green btn-sm dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                İşlemler
                <i class="fa fa-angle-down"></i>
            </button>
            <ul class="dropdown-menu pull-right" role="menu">
                <li>
                    <a href="/SiparisGonderim/Save">
                        <i class="fa fa-save"></i> Yeni Kayıt
                    </a>
                </li>
                <li class="divider"></li>
                <li>
                    <a href="" class="fullscreen" data-original-title="Tam Ekran" title="Tam Ekran">
                        <i class="fa fa-arrows-alt"></i> Tam Ekran
                    </a>
                </li>
            </ul>
        </div>
    </div>
    <div class="portlet-body">
        <div class="portlet light bordered">
            <div class="portlet-title">
                <div class="caption">
                    <i class="fa fa-filter font-dark"></i>
                    <span class="caption-subject font-dark bold">Filtreleme Kriterleri</span>
                </div>
                <div class="tools">
                    <a href="" class="collapse" data-original-title="Genişlet / Daralt" title="Genişlet / Daralt"> </a>
                </div>
            </div>
            <div class="portlet-body form">
                <form action="#" class="form-horizontal" role="form">
                    <div class="form-body">
                        <div class="form-group">
                            <div class="col-md-6">
                                <label for="dropDownIl" class="control-label">Gönderim Adresi İl</label>
                                <select id="dropDownIl" name="dropDownIl" class="form-control select2" style="width:100%;" tabindex="1"></select>
                            </div>
                            <div class="col-md-6">
                                <label for="dropDownIlce" class="control-label">Gönderim Adresi İlçe</label>
                                <select id="dropDownIlce" name="dropDownIlce" class="form-control select2" style="width:100%;" tabindex="2"></select>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-6">
                                <label for="dropDownGonderimTipi" class="control-label">Gönderim Tipi</label>
                                <select id="dropDownGonderimTipi" name="dropDownGonderimTipi" class="form-control select2" style="width:100%;" tabindex="3"></select>
                            </div>
                            <div class="col-md-6">
                                <label for="dropDownTeslimZamaniTipi" class="control-label">Teslim Zamanı Tipi</label>
                                <select id="dropDownTeslimZamaniTipi" name="dropDownTeslimZamaniTipi" class="form-control select2" style="width:100%;" tabindex="4"></select>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-6">
                                <label for="txtTakipNo" class="control-label">Takip No</label>
                                <input id="txtTakipNo" name="txtTakipNo" class="form-control input-sm maxlength-handler" maxlength="50" placeholder="Takip No Giriniz..." tabindex="5">
                            </div>
                            <div class="col-md-6">
                                <label for="txtAciklama" class="control-label">Açıklama</label>
                                <input id="txtAciklama" name="txtAciklama" class="form-control input-sm maxlength-handler" maxlength="500" placeholder="Açıklama Giriniz..." tabindex="6">
                            </div>                            
                        </div>
                        <div class="form-group">
                            <div class="col-md-6">
                                <label class="control-label">Gönderim Tarihi</label>
                                <div class="col-md-12" style="padding-left:0px !important;padding-right:0px !important;">
                                    <div class="col-md-6" style="padding-left:0px !important;">
                                        <div id="dateTimeGonderimBaslangic" class="input-group date" data-provide="datepicker">
                                            <input type="text" class="form-control" placeholder="Başlangıç Tarihi" tabindex="7">
                                            <div class="input-group-addon">
                                                <span class="glyphicon glyphicon-th"></span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6" style="padding-right:0px !important;">
                                        <div id="dateTimeGonderimBitis" class="input-group date" data-provide="datepicker">
                                            <input type="text" class="form-control" placeholder="Bitiş Tarihi" tabindex="8">
                                            <div class="input-group-addon">
                                                <span class="glyphicon glyphicon-th"></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <label class="control-label">Teslim Tarihi</label>
                                <div class="col-md-12" style="padding-left:0px !important;padding-right:0px !important;">
                                    <div class="col-md-6" style="padding-left:0px !important;">
                                        <div id="dateTimeTeslimBaslangic" class="input-group date" data-provide="datepicker">
                                            <input type="text" class="form-control" placeholder="Başlangıç Tarihi" tabindex="9">
                                            <div class="input-group-addon">
                                                <span class="glyphicon glyphicon-th"></span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6" style="padding-right:0px !important;">
                                        <div id="dateTimeTeslimBitis" class="input-group date" data-provide="datepicker">
                                            <input type="text" class="form-control" placeholder="Bitiş Tarihi" tabindex="10">
                                            <div class="input-group-addon">
                                                <span class="glyphicon glyphicon-th"></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-6">
                                <label class="control-label">Sipariş Tarihi</label>
                                <div class="col-md-12" style="padding-left:0px !important;padding-right:0px !important;">
                                    <div class="col-md-6" style="padding-left:0px !important;">
                                        <div id="dateTimeSiparisBaslangic" class="input-group date" data-provide="datepicker">
                                            <input type="text" class="form-control" placeholder="Başlangıç Tarihi" tabindex="11">
                                            <div class="input-group-addon">
                                                <span class="glyphicon glyphicon-th"></span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6" style="padding-right:0px !important;">
                                        <div id="dateTimeSiparisBitis" class="input-group date" data-provide="datepicker">
                                            <input type="text" class="form-control" placeholder="Bitiş Tarihi" tabindex="12">
                                            <div class="input-group-addon">
                                                <span class="glyphicon glyphicon-th"></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <label class="control-label">Aktif mi?</label>
                                <div class="input-group">
                                    <div class="icheck-inline">
                                        <label>
                                            <input type="radio" data-radio="iradio_square-aero" name="rdAktif" class="icheck" value="2"> Tümü
                                        </label>
                                        <label>
                                            <input type="radio" data-radio="iradio_square-aero" name="rdAktif" checked class="icheck" value="1"> Aktif
                                        </label>
                                        <label>
                                            <input type="radio" data-radio="iradio_square-aero" name="rdAktif" class="icheck" value="0"> Pasif
                                        </label>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="form-actions">
                            <div class="col-md-offset-5 col-md-7">
                                <button type="button" class="btn blue-madison" onclick="javascript: ara()" tabindex="13">
                                    Ara
                                    <i class="fa fa-search"></i>
                                </button>
                                <button type="button" class="btn green-meadow" onclick="javascript: temizle()" tabindex="14">
                                    Temizle
                                    <i class="fa fa-recycle"></i>
                                </button>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>

        <div class="portlet light bordered">
            <div class="portlet-title">
                <div class="caption font-dark">
                    <i class="fa fa-list-ul font-dark"></i>
                    <span class="caption-subject bold">Sonuç Listesi</span>
                </div>
            </div>
            <div class="portlet-body">
                <div class="box">
                    <div class="box-header">
                        <div id="boxTools" class="box-tools" style="display:none;">
                            <div class="input-group pull-left">
                                <input type="hidden" id="dropDownPagingSize" class="form-control select2" style="width: 100px;" value="5" />
                            </div>
                            <div class="input-group pull-right">
                                <button type="button" class="btn btn-xs green-stripe pull-right" style="margin-right:10px;" onclick="javascript: excelRaporuAl();"><i class="fa fa-file-excel-o"></i>Excel</button>
                            </div>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <div class="box-body">
                        <div class="table-responsive">
                            <table id="tableSiparisGonderim" class="table table-striped table-bordered table-hover">
                                <thead>
                                    <tr>
                                        <th width="6%">Takip No</th>
                                        <th width="5%">Adet</th>
                                        <th width="8%">Ürün</th>
                                        <th width="10%">Gönderim İlçe/İl</th>
                                        <th width="10%">Gönderim Tipi</th>
                                        <th width="10%">Teslim Zamanı Tipi</th>
                                        <th width="13%">Açıklama</th>
                                        <th width="8%">Gönderim Tarihi</th>
                                        <th width="8%">Teslim Tarihi</th>
                                        <th width="8%">Sipariş Tarihi</th>
                                        <th width="7%">Aktif mi?</th>
                                        <th width="7%">İşlemler</th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                        <div id="footerInfo" class="row" style="display:none;">
                            <div class="col-md-12">
                                <div class="input-group pull-left">
                                    <p id="tableInfo" style="color: #6f6f6f;font-size: 12px;"></p>
                                </div>
                                <div class="input-group pull-right">
                                    <ul id="ulPaging" class="no-margin pull-right"></ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@section js{
    <script id="row" type="text/x-jquery-tmpl">
        <tr>
            <td>${TakipNo}</td>
            <td>${Adet}</td>
            <td>
                <a href="/Urun/Card/${UrunId}" target="_blank" class="btn btn-xs purple-plum" style="width:100%;" data-toggle="tooltip" data-placement="top" title="Ürüne Git">
                    <i class="fa fa-share"></i>
                </a>
            </td>
            <td>${IlceAdi}/${IlAdi}</td>
            <td>${GonderimTipi}</td>
            <td>${TeslimZamaniTipi}</td>
            <td>${Aciklama}</td>
            <td>${GonderimTarihi}</td>
            <td>${TeslimTarihi}</td>
            <td>${SiparisTarihi}</td>
            <td>
                <div class="icheck-inline">
                    {{if AktifMi}}
                        <input type="checkbox" class="make-switch" data-size="mini" data-on-text="Aktif" data-off-text="Pasif" checked readonly>
                    {{else}}
                        <input type="checkbox" class="make-switch" data-size="mini" data-on-text="Aktif" data-off-text="Pasif" readonly>
                    {{/if}}
                </div>
            </td>
            <td>
                <div class="text-center">
                    <a href="javascript: sil('${SiparisGonderimId}');" class="btn btn-xs red" data-toggle="tooltip" data-placement="top" title="Sil">
                        <i class="fa fa-remove"></i>
                    </a>
                    <a href="/SiparisGonderim/Save/${SiparisGonderimId}" class="btn btn-xs yellow-casablanca" data-toggle="tooltip" data-placement="top" title="Güncelle">
                        <i class="fa fa-edit"></i>
                    </a>
                </div>
            </td>
        </tr>
    </script>

    <script type="text/javascript">
        var gonderimTipListesi = @Html.Raw(Json.Encode(ViewBag.GonderimTipListesi))
        var teslimZamaniTipListesi = @Html.Raw(Json.Encode(ViewBag.TeslimZamaniTipListesi))
        var ilListesi = @Html.Raw(Json.Encode(ViewBag.IlListesi))
        var ilceListesi = @Html.Raw(Json.Encode(ViewBag.IlceListesi))

        $(document).ready(function () {
            $('.input-group.date').datepicker({
                autoclose: true,
                dateFormat: "dd.mm.yyyy",
                language: 'tr'
            });

            $('#dropDownGonderimTipi').select2({
                placeholder: "Gönderim Tipi Seçiniz...",
                allowClear: true,
                language: {
                    noResults: function (params) {
                        return "Hiç kayıt bulunamadı.";
                    }
                },
                data: gonderimTipListesi,
                escapeMarkup: function (markup) {
                    return markup;
                },
                templateResult: templateData,
                templateSelection: templateDataSelection
            }).val('').trigger('change');

            $('#dropDownTeslimZamaniTipi').select2({
                placeholder: "Teslim Zamanı Tipi Seçiniz...",
                allowClear: true,
                language: {
                    noResults: function (params) {
                        return "Hiç kayıt bulunamadı.";
                    }
                },
                data: teslimZamaniTipListesi,
                escapeMarkup: function (markup) {
                    return markup;
                },
                templateResult: templateData,
                templateSelection: templateDataSelection
            }).val('').trigger('change');

            $('#dropDownIlce').select2({
                placeholder: "İlçe Seçiniz...",
                disabled: true,
                allowClear: true,
                language: {
                    noResults: function (params) {
                        return "Hiç kayıt bulunamadı.";
                    }
                },
                data: null,
                escapeMarkup: function (markup) {
                    return markup;
                },
                templateResult: templateData,
                templateSelection: templateDataSelection
            }).val('').trigger('change');

            $('#dropDownIl').select2({
                placeholder: "İl Seçiniz...",
                allowClear: true,
                language: {
                    noResults: function (params) {
                        return "Hiç kayıt bulunamadı.";
                    }
                },
                data: ilListesi,
                escapeMarkup: function (markup) {
                    return markup;
                },
                templateResult: templateData,
                templateSelection: templateDataSelection
            }).on('change', function () {
                if ($('#dropDownIl').val() != null && $('#dropDownIl').val() != '') {
                    var chsnIlId = parseInt($('#dropDownIl').val());
                    var ilceler = null;
                    ilceler = $.grep(ilceListesi, function (a) {
                        return a.ilId === chsnIlId;
                    });

                    $('#dropDownIlce').select2('destroy');
                    $('#dropDownIlce').empty();

                    $('#dropDownIlce').select2({
                        placeholder: "İlçe Seçiniz...",
                        disabled: false,
                        allowClear: true,
                        language: {
                            noResults: function (params) {
                                return "Hiç kayıt bulunamadı.";
                            }
                        },
                        data: ilceler,
                        escapeMarkup: function (markup) {
                            return markup;
                        },
                        templateResult: templateData,
                        templateSelection: templateDataSelection
                    }).val('').trigger('change');
                } else {
                    $('#dropDownIlce').select2('destroy');
                    $('#dropDownIlce').empty();

                    $('#dropDownIlce').select2({
                        placeholder: "İlçe Seçiniz...",
                        disabled: true,
                        allowClear: true,
                        language: {
                            noResults: function (params) {
                                return "Hiç kayıt bulunamadı.";
                            }
                        },
                        data: null,
                        escapeMarkup: function (markup) {
                            return markup;
                        },
                        templateResult: templateData,
                        templateSelection: templateDataSelection
                    }).val('').trigger('change');
                }
            }).val('').trigger('change');

            $('#dropDownPagingSize').select2({
                language: {
                    noResults: function (params) {
                        return "Hiç kayıt bulunamadı.";
                    }
                },
                data: [
                    { id: 5, text: 5 },
                    { id: 10, text: 10 },
                    { id: 20, text: 20 },
                    { id: 50, text: 50 }
                ]
            }).on('select2:select', function (e) {
                ara(1, $('#dropDownPagingSize').val());
            });
        });

        function templateData(obj) {
            if (obj.id == '')
                return obj.text;

            if (obj.aktif) {
                return obj.text;
            } else {
                return '<span style="color:red;">' + obj.text + ' (Pasif)</span>';
            }
        }
        function templateDataSelection(obj) {
            if (obj.id == '')
                return obj.text;

            if (obj.aktif) {
                return obj.text;
            } else {
                return '<span style="color:red;">' + obj.text + ' (Pasif)</span>';
            }
        }

        function paging(totalCount, pageCount, pageIndex) {
            $('#ulPaging').pagination('destroy');
            $('#ulPaging').pagination({
                items: totalCount,
                itemsOnPage: pageCount,
                currentPage: pageIndex,
                cssStyle: 'light-theme',
                prevText: 'Önceki Sayfa',
                nextText: 'Sonraki Sayfa',
                onPageClick: function (pageNum) {
                    ara(pageNum, pageCount);
                }
            });
        }

        function ara(pageIndex, pageCount) {
            if ($('#dateTimeGonderimBaslangic').datepicker('getDate') != '' && $('#dateTimeGonderimBitis').datepicker('getDate')) {
                if ($('#dateTimeGonderimBaslangic').datepicker('getDate') > $('#dateTimeGonderimBitis').datepicker('getDate')) {
                    callAlert('warning', 'Gönderim başlangıç tarihi bitiş tarihinden büyük olamaz.');
                    return false;
                }
            }

            if ($('#dateTimeTeslimBaslangic').datepicker('getDate') != '' && $('#dateTimeTeslimBitis').datepicker('getDate')) {
                if ($('#dateTimeTeslimBaslangic').datepicker('getDate') > $('#dateTimeTeslimBitis').datepicker('getDate')) {
                    callAlert('warning', 'Teslim başlangıç tarihi bitiş tarihinden büyük olamaz.');
                    return false;
                }
            }

            if ($('#dateTimeSiparisBaslangic').datepicker('getDate') != '' && $('#dateTimeSiparisBitis').datepicker('getDate')) {
                if ($('#dateTimeSiparisBaslangic').datepicker('getDate') > $('#dateTimeSiparisBitis').datepicker('getDate')) {
                    callAlert('warning', 'Sipariş başlangıç tarihi bitiş tarihinden büyük olamaz.');
                    return false;
                }
            }

            pageIndex = pageIndex == null ? 1 : pageIndex;
            pageCount = pageCount == null ? parseInt($('#dropDownPagingSize').val()) : pageCount;

            $.ajax({
                url: '/SiparisGonderim/Ara',
                data: {
                    'gonderimAdresIlId': ($('#dropDownIl').val() != '' && $('#dropDownIl').val() != null) ? $('#dropDownIl').val() : 0,
                    'gonderimAdresIlceId': ($('#dropDownIlce').val() != '' && $('#dropDownIlce').val() != null) ? $('#dropDownIlce').val() : 0,
                    'gonderimTipId': ($('#dropDownGonderimTipi').val() != '' && $('#dropDownGonderimTipi').val() != null) ? $('#dropDownGonderimTipi').val() : 0,
                    'teslimZamaniTipId': ($('#dropDownTeslimZamaniTipi').val() != '' && $('#dropDownTeslimZamaniTipi').val() != null) ? $('#dropDownTeslimZamaniTipi').val() : 0,
                    'takipNo': $('#txtTakipNo').val(),
                    'aciklama': $('#txtAciklama').val(),
                    'gonderimBaslangic': $('#dateTimeGonderimBaslangic').find('input').val() != '' ? $('#dateTimeGonderimBaslangic').datepicker('getDate').toUTCString() : null,
                    'gonderimBitis': $('#dateTimeGonderimBitis').find('input').val() != '' ? $('#dateTimeGonderimBitis').datepicker('getDate').toUTCString() : null,
                    'teslimBaslangic': $('#dateTimeTeslimBaslangic').find('input').val() != '' ? $('#dateTimeTeslimBaslangic').datepicker('getDate').toUTCString() : null,
                    'teslimBitis': $('#dateTimeTeslimBitis').find('input').val() != '' ? $('#dateTimeTeslimBitis').datepicker('getDate').toUTCString() : null,
                    'siparisBaslangic': $('#dateTimeSiparisBaslangic').find('input').val() != '' ? $('#dateTimeSiparisBaslangic').datepicker('getDate').toUTCString() : null,
                    'siparisBitis': $('#dateTimeSiparisBitis').find('input').val() != '' ? $('#dateTimeSiparisBitis').datepicker('getDate').toUTCString() : null,
                    'aktiflik': parseInt($('input[name=rdAktif]:checked').val()),
                    'sayfaSayisi': pageCount,
                    'sayfaSirasi': pageIndex
                },
                type: "get",
                dataType: "json",
                contentType: "application/json; charset=utf-8",
                beforeSend: function () {
                    showLoadingModal();
                },
                success: function (result) {
                    hideLoadingModal();

                    $('#tableSiparisGonderim tbody').children().remove();
                    $('#row').tmpl(result.sonucListesi).appendTo('#tableSiparisGonderim tbody');

                    $('#tableSiparisGonderim .make-switch').bootstrapSwitch();
                    $('[data-toggle="tooltip"]').tooltip();

                    // table info
                    var txt = '';
                    if (result.toplamKayitSayisi > 0) {
                        var firstPart = ((result.sayfaSirasi - 1) * (result.sayfaSayisi) + 1);
                        var secondPart = (result.sayfaSirasi * result.sayfaSayisi) > result.toplamKayitSayisi ? result.toplamKayitSayisi : (result.sayfaSirasi * result.sayfaSayisi);
                        txt = 'Toplam ' + result.toplamKayitSayisi + ' kayıttan ' + firstPart + '-' + secondPart + ' gösteriliyor.';

                        $('#boxTools').show();
                        $('#footerInfo').show();
                    }
                    else {
                        txt = 'Hiç kayıt bulunamadı.';

                        $('#boxTools').hide();
                        $('#footerInfo').hide();
                    }
                    $('#tableInfo').text(txt);

                    paging(result.toplamKayitSayisi, result.sayfaSayisi, result.sayfaSirasi);
                },
                error: function () {
                    hideLoadingModal();
                    callAlert('error', 'Beklenmedik bir sorun oluştu. Lütfen tekrar deneyin.');
                }
            });
        }

        function temizle() {
            $('#dropDownIl').val('').trigger('change');
            $('#dropDownIlce').val('').trigger('change');
            $('#dropDownGonderimTipi').val('').trigger('change');
            $('#dropDownTeslimZamaniTipi').val('').trigger('change');
            $('#txtTakipNo').val('');
            $('#txtAciklama').val('');
            $('#dateTimeGonderimBaslangic').data('datepicker').setDate(null);
            $('#dateTimeGonderimBitis').data('datepicker').setDate(null);
            $('#dateTimeTeslimBaslangic').data('datepicker').setDate(null);
            $('#dateTimeTeslimBitis').data('datepicker').setDate(null);
            $('#dateTimeSiparisBaslangic').data('datepicker').setDate(null);
            $('#dateTimeSiparisBitis').data('datepicker').setDate(null);
        }

        function sil(id) {
            confirm('ONAY', '<p>Silmek istediğinize emin misiniz?</p>', function () {
                $.ajax({
                    url: '/SiparisGonderim/Sil/' + id,
                    type: "post",
                    dataType: "json",
                    contentType: "application/json; charset=utf-8",
                    beforeSend: function () {
                        showLoadingModal();
                    },
                    success: function (result) {
                        hideLoadingModal();
                        if (result) {
                            callAlert('success', 'Silme işlemi başarılı.');
                            ara();
                        }
                        else {
                            callAlert('error', 'İşlem sırasında bir sorun oluştu. Lütfen tekrar deneyin.');
                        }
                    },
                    error: function () {
                        hideLoadingModal();
                        callAlert('error', 'Beklenmedik bir sorun oluştu. Lütfen tekrar deneyin.');
                    }
                });
            });
        }

        function excelRaporuAl() {
            var gonderimAdresIlId = ($('#dropDownIl').val() != '' && $('#dropDownIl').val() != null) ? $('#dropDownIl').val() : 0;
            var gonderimAdresIlceId = ($('#dropDownIlce').val() != '' && $('#dropDownIlce').val() != null) ? $('#dropDownIlce').val() : 0;
            var gonderimTipId = ($('#dropDownGonderimTipi').val() != '' && $('#dropDownGonderimTipi').val() != null) ? $('#dropDownGonderimTipi').val() : 0;
